<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>GetLocalidadesServlet</servlet-name>
        <servlet-class>servlets.GetLocalidadesServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetPromocionesServlet</servlet-name>
        <servlet-class>servlets.GetPromocionesServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetTiposInmuebleServlet</servlet-name>
        <servlet-class>servlets.GetTiposInmuebleServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetInmueblesServlet</servlet-name>
        <servlet-class>servlets.GetInmueblesServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GenerarMapaServlet</servlet-name>
        <servlet-class>servlets.GenerarMapaServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ActualizaInmuebleServlet</servlet-name>
        <servlet-class>servlets.ActualizaInmuebleServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GetLocalidadesServlet</servlet-name>
        <url-pattern>/GetLocalidadesServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetPromocionesServlet</servlet-name>
        <url-pattern>/GetPromocionesServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetTiposInmuebleServlet</servlet-name>
        <url-pattern>/GetTiposInmuebleServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetInmueblesServlet</servlet-name>
        <url-pattern>/GetInmueblesServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GenerarMapaServlet</servlet-name>
        <url-pattern>/GenerarMapaServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ActualizaInmuebleServlet</servlet-name>
        <url-pattern>/ActualizaInmuebleServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
